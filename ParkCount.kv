
<ParkCount>:
	name: 'Park Count'
    canvas.before:
        Color:
            rgba: 0.8, 1, 0.698, 1
        Rectangle:
            pos: self.pos
            size: self.size
	BoxLayout:
		pos: 55, 600
		size_hint: 0.8, 0.2
        orientation:  'vertical'
		Label:
			text:'Insert number of parks cleaned up below'
			color: "black"
    BoxLayout:
        pos: 55, 400
        size_hint: 0.8, 0.2
        TextInput: 
            text: 'Enter here'
            id: Submit2
            size: self.size 
    BoxLayout:
        pos:55, 200
        size_hint: 0.8, 0.2
		Button: 
            text: 'Enter'
            on_press:
                root.addInfo()
            background_color: 0.0, 0.447, 0.071, 1
    Button:
        text: 'Back'
		on_press:
			root.manager.current = 'CollectiveImpact'
		size_hint: 0.16, 0.1 
		pos: 0, 0
		
        background_color: 0.0, 0.447, 0.071, 1

