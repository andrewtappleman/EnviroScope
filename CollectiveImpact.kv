<CollectiveImpact>:
    name: "CollectiveImpact"
    canvas.before:
        Color:
            rgba: 0.592, 0.741, 0.969, 1
        Rectangle:
            pos: self.pos
            size: self.size
	BoxLayout:
		pos: 55, 100
		size_hint: 0.8, 0.8
		orientation:  'vertical'
		Button:
			text: 'Total Litter Cleaned up'
			on_press:
				root.manager.current = 'outOrder'
			size: self.texture_size
			pos: 45, 675
			background_color: 0.4196, 0.7922, 0.9569, 1
		
		Button:
			text: 'Total Bottles Picked up'
			on_press:
				root.manager.current = 'BottleCount'
			size: self.texture_size
			pos: 45, 550
			background_color: 0.4196, 0.7922, 0.9569, 1
		
		Button:
			text: 'Total Parks Cleaned'
			on_press:
				root.manager.current = 'BottleCount'
			size: self.texture_size
			pos: 45, 425
			background_color: 0.4196, 0.7922, 0.9569, 1
	
	Button:
		text: 'Back'
		on_press:
			root.manager.current = 'GetInspired'
		size_hint: 0.16, 0.1
		pos: 0, 0
		background_color: 0.4196, 0.7922, 0.9569, 1