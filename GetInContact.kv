<GetInContact>:
    name: "GetInContact"
	canvas.before:
		Color:
			rgba: 0.8, 1, 0.698, 1
		Rectangle:
			pos: self.pos
			size: self.size
	Button:
        id: dropButton1
        text: 'Choose a State'
        pos: 55, 600
        size_hint: 0.8, 0.1
        on_press: root.createState()
        background_color: 0.0, 0.447, 0.071, 1
	
	Button:
        id: dropButton2
        text: 'Choose a District'
        pos: 55, 400
        size_hint: 0.8, 0.1
        on_press: root.createDistrict()
        background_color: 0.0, 0.447, 0.071, 1

    Button:
        text: 'Submit'
        size_hint: 0.8, 0.1
        pos: 55, 200
        background_color: 0.0, 0.447, 0.071, 1
        on_press: root.access()
		
    Button:
		text: 'Back'
		on_press:
			root.manager.current = 'EnvironmentalIssues'
		size_hint: 0.16, 0.1
		pos: 0, 0
		background_color: 0.0, 0.447, 0.071, 1
