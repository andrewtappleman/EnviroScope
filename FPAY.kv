<FPAY>:
    name: "FPAY"
    canvas.before:
        Color:
            rgba: 0.592, 0.741, 0.969, 1
        Rectangle:
            pos: self.pos
            size: self.size

    
    BoxLayout:
        pos: 55, 625
        size_hint: 0.8, 0.15
        TextInput:
            id: zipcode
            text: 'Zipcode'
            pos: self.pos
            size: self.size
    BoxLayout:
        pos: 55, 450
        size_hint: 0.8, 0.15
        Button:
            text: 'Open Links'
            pos: self.pos
            size: self.size
            on_press: root.makeLink()
            background_color: 0.4196, 0.7922, 0.9569, 1

    BoxLayout:
        pos: 55, 275
        size_hint: 0.8, 0.15
        Button:
            text: 'Submit'
            pos: self.pos
            size: self.texture_size
            background_color: 0.4196, 0.7922, 0.9569, 1
            on_press: root.perform_search()
                
    Button:
        text: 'Back'
        pos: 0, 0
        size_hint: 0.16, 0.1
        on_press:
            root.manager.current = 'GetInformed'
        background_color: 0.4196, 0.7922, 0.9569, 1